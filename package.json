{
  "name": "transfer-frontend",
  "version": "1.0.0",
  "description": "'Frontend for QR Code Transfer'",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.prod.js",
    "start": "webpack serve --config ./config/webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Transfer-team/QRCodeTransfer.git"
  },
  "keywords": [
    "React",
    "QR"
  ],
  "author": "Xavier Garcia <xaviergn60@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Transfer-team/QRCodeTransfer/issues"
  },
  "homepage": "https://github.com/Transfer-team/QRCodeTransfer#readme",
  "dependencies": {
    "core-js": "^3.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "mini-css-extract-plugin": "^1.3.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-refresh": "^0.9.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "browserslist": ">0.25%, not dead, not ie 11",
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>./setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/./src/__mocks__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run test",
      "pre-push": "npm test"
    }
  }
}
